{
  "routes": [
    {
      "source": "^/health/ping$",
      "target": "/healthCheck.json",
      "localDir": "resources",
      "authenticationType": "none",
      "csrfProtection": false
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "authenticationType": "xsuaa",
      "scope": "$XSAPPNAME.Display",
      "csrfProtection": false,
      "destination": "ac-kowl"
    }
  ],
  "logout": {
    "logoutEndpoint": "/logout",
    "logoutPage": "/logged-out.html"
  }
}